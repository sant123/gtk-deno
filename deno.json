{
  "lock": false,
  "name": "@onyx/gtk",
  "version": "0.0.2",
  "publish": {
    "exclude": [
      ".clang-format",
      ".github",
      ".gitmodules",
      ".vscode",
      "examples",
      "main.c",
      "tests",
      "third_party"
    ]
  },
  "exports": {
    ".": "./lib/version.ts",
    "./GtkApplication": "./src/GtkApplication/mod.ts",
    "./GtkApplicationWindow": "./src/GtkApplicationWindow/mod.ts",
    "./GtkFileDialog": "./src/GtkFileDialog/mod.ts",
    "./GtkFileFilter": "./src/GtkFileFilter/mod.ts"
  },
  "imports": {
    "@std/assert": "jsr:@std/assert@^1.0.11",
    "@std/async": "jsr:@std/async@^1.0.12",
    "@webview/webview": "jsr:@webview/webview@^0.9.0",
    "lib": "./lib/mod.ts",
    "loop": "./src/loop.ts",
    "signal": "./src/signal/mod.ts",
    "types": "./src/types.ts",
    "utils": "./src/utils.ts",
    "valibot": "jsr:@valibot/valibot@^1.0.0"
  },
  "fmt": {
    "exclude": ["gtk4", "third_party"]
  },
  "lint": {
    "exclude": ["gtk4", "third_party"]
  },
  "compilerOptions": {
    "lib": [
      "deno.worker"
    ]
  }
}
